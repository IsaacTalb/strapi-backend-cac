{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "type": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": "news",
      "enum": [
        "news",
        "technicality",
        "report"
      ]
    },
    "newsCategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::news-category.news-category",
      "inversedBy": "posts"
    },
    "technicalityCategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::technicality-category.technicality-category",
      "inversedBy": "posts"
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": "Set your title here",
      "required": true
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": "Set your description here",
      "required": true
    },
    "slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "targetField": "",
      "required": true
    },
    "tags": {
      "type": "customField",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "customField": "plugin::tagsinput.tags",
      "options": {
        "apiUrl": "/api/tags?fields[0]=name"
      }
    },
    "content": {
      "type": "customField",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      },
      "required": true
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "posts"
    },
    "timeRead": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "cover": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "order": {
      "type": "biginteger",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false
    },
    "reportYear": {
      "type": "biginteger",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false
    },
    "publishedDate": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    }
  }
}
